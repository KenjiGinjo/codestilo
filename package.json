{
  "name": "codestilo",
  "version": "0.0.4",
  "description": "Codestilo - A unified toolkit for managing code style and linting rules",
  "keywords": [
    "eslint",
    "prettier",
    "style",
    "lint",
    "code",
    "typescript"
  ],
  "author": "Kenji <kenjiginjo@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kenjiginjo/codestilo.git"
  },
  "bugs": {
    "url": "https://github.com/kenjiginjo/codestilo/issues"
  },
  "main": "./build/index.js",
  "type": "module",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "build": "bun build ./src/index.js --outdir ./build --target=node",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@eslint/js": "^9.1.1",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "prettier-plugin-multiline-arrays": "^3.0.4",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.14",
    "prettier-plugin-prisma": "^5.0.0",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "peerDependencies": {
    "eslint": ">= 9"
  }
}
